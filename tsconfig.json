{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "resolveJsonModule": true,
    "outDir": "./dist",
    "strict": true,
    "noImplicitAny": false,
    "paths": {
      "@app/*":["./*"]
    },
    "baseUrl": "./",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "plugins": [
      {
        "transform": "typescript-transform-paths"
      }
    ]
  }
}
